{
    "name": "lunar-static-pages/addon",
    "version": "0.1.2",
    "type": "project",
    "description": "Static Pages functionality for Lunar.",
    "keywords": [
        "lunarphp",
        "laravel",
        "ecommerce",
        "e-commerce",
        "headless",
        "store",
        "shop",
        "cart",
        "stripe"
    ],
    "license": "MIT",
    "source": {
        "type": "git",
        "url": "https://github.com/SteliyanPStoyanov/lunar-static-page-addon.git",
        "reference": ""
    },
    "authors": [
        {
            "name": "Steliyan Stoyanov",
            "homepage": "https://github.com/SteliyanPStoyanov"
        }
    ],
    "require": {
        "php": "^8.0",
        "lunarphp/lunar": "^0.6",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lunar\\StaticPages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lunar\\StaticPages\\Tests\\": "tests/"
        }
    },
    "extra": {
        "lunar": {
            "name": "Static Pages"
        },
        "laravel": {
            "providers": [
                "Lunar\\StaticPages\\PageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
